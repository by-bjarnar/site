{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ci": "payload migrate && pnpm build",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "i:build": "infisical run pnpm build",
    "build:clean": "rm -rf .next && rm -rf next-env.d.ts && pnpm build",
    "i:build:clean": "infisical run pnpm build:clean",
    "dev": "next dev --turbo",
    "i:dev": "infisical run pnpm dev",
    "dev:clean": "rm -rf .next && rm -rf next-env.d.ts && pnpm dev",
    "i:dev:clean": "infisical run pnpm dev:clean",
    "start": "next start",
    "i:start": "infisical run pnpm start",
    "migrate": "payload migrate",
    "i:migrate": "infisical run pnpm migrate",
    "migrate:create": "payload migrate:create",
    "i:migrate:create": "infisical run pnpm migrate:create",
    "migrate:clean": "rm -rf src/payload/migrations/* && touch src/payload/migrations/.gitkeep",
    "migrate:dev": "pnpm migrate:clean && pnpm i:migrate:create && pnpm i:migrate",
    "generate:importmap": "infisical run payload generate:importmap",
    "generate:types": "infisical run payload generate:types",
    "lint": "next lint",
    "lint:all": "pnpm prettier && pnpm eslint && pnpm stylelint",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "prettier": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "stylelint": "stylelint '**/*.css' --ignore-path .gitignore",
    "stylelint:fix": "stylelint '**/*.css' --fix --ignore-path .gitignore"
  },
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "@payloadcms/db-postgres": "3.29.0",
    "@payloadcms/email-resend": "3.29.0",
    "@payloadcms/next": "3.29.0",
    "@payloadcms/plugin-nested-docs": "3.29.0",
    "@payloadcms/richtext-lexical": "3.29.0",
    "@payloadcms/storage-s3": "3.29.0",
    "@payloadcms/ui": "3.29.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "graphql": "^16.8.1",
    "next": "15.2.3",
    "payload": "3.29.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@next/eslint-plugin-next": "15.2.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.16.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.27.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
